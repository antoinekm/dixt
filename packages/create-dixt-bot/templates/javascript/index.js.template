const dixt = require("dixt").default;
{{#if createExamplePlugin}}
const myPlugin = require("../plugins/my-plugin.js").default;
{{/if}}

const bot = new dixt({
  application: {
    name: "{{projectName}}",
  },{{#if createExamplePlugin}}
  plugins: [myPlugin],{{/if}}
});

bot.start().then(() => {
  console.log("Bot is running!");
});
