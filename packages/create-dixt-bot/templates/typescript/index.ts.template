import dixt from "dixt";
{{#if createExamplePlugin}}
import myPlugin from "../plugins/my-plugin.js";
{{/if}}

const bot = new dixt({
  application: {
    name: "{{projectName}}",
  },{{#if createExamplePlugin}}
  plugins: [myPlugin],{{/if}}
});

(async () => {
  await bot.start();
  console.log("Bot is running!");
})();
